



(defrule
    (can-research ri-guilds)
    =>
    (research ri-guilds)
)


(defrule
    (can-research ri-banking)
    =>
    (research ri-banking)
)


(defrule
    (current-age < imperial-age)
    =>
    (up-jump-rule 2)
)

(defrule
    (player-in-game any-ally)
    (can-research ri-coinage)
    =>
    (research ri-coinage)
)


(defrule
    (player-in-game any-ally)
    (can-research ri-caravan)
    =>
    (research ri-caravan)
)
